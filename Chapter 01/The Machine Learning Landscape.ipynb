      "source": [
        "# Reproduced Code - Hands-On ML Chapter 1\n",
        "# Data source: Official GitHub (Aurélien Géron)\n",
        "\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "from sklearn.linear_model import LinearRegression\n",
        "from sklearn.neighbors import KNeighborsRegressor\n",
        "\n",
        "# Load data directly from GitHub\n",
        "OECD_URL = \"https://raw.githubusercontent.com/ageron/handson-ml/master/datasets/lifesat/oecd_bli_2015.csv\"\n",
        "GDP_URL  = \"https://raw.githubusercontent.com/ageron/handson-ml/master/datasets/lifesat/gdp_per_capita.csv\"\n",
        "\n",
        "oecd_bli = pd.read_csv(OECD_URL, thousands=',')\n",
        "gdp_per_capita = pd.read_csv(\n",
        "    GDP_URL,\n",
        "    thousands=',',\n",
        "    delimiter='\\t',\n",
        "    encoding='latin1',\n",
        "    na_values=\"n/a\"\n",
        ")\n",
        "\n",
        "# Data preparation (re-implemented)\n",
        "def prepare_country_stats(oecd, gdp):\n",
        "    oecd = oecd[oecd[\"INEQUALITY\"] == \"TOT\"]\n",
        "    oecd = oecd.pivot(index=\"Country\", columns=\"Indicator\", values=\"Value\")\n",
        "    gdp = gdp.rename(columns={\"2015\": \"GDP per capita\"})\n",
        "    gdp = gdp.set_index(\"Country\")\n",
        "\n",
        "    full_data = pd.merge(oecd, gdp, left_index=True, right_index=True)\n",
        "    full_data = full_data[[\"GDP per capita\", \"Life satisfaction\"]]\n",
        "    full_data = full_data.dropna()\n",
        "\n",
        "    return full_data.sort_values(by=\"GDP per capita\")\n",
        "\n",
        "country_stats = prepare_country_stats(oecd_bli, gdp_per_capita)\n",
        "\n",
        "# Feature & target\n",
        "X = country_stats[[\"GDP per capita\"]].values\n",
        "y = country_stats[[\"Life satisfaction\"]].values\n",
        "\n",
        "# Visualization\n",
        "plt.figure(figsize=(8, 5))\n",
        "plt.scatter(X, y)\n",
        "plt.xlabel(\"GDP per capita\")\n",
        "plt.ylabel(\"Life satisfaction\")\n",
        "plt.title(\"GDP vs Life Satisfaction\")\n",
        "plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "# Linear Regression (model-based learning)\n",
        "lin_model = LinearRegression()\n",
        "lin_model.fit(X, y)\n",
        "\n",
        "X_new = [[22587]]  # Cyprus GDP per capita\n",
        "lin_pred = lin_model.predict(X_new)\n",
        "\n",
        "print(\"Linear Regression prediction (Cyprus):\", lin_pred[0][0])\n",
        "\n",
        "# KNN Regression (instance-based learning)\n",
        "knn_model = KNeighborsRegressor(n_neighbors=3)\n",
        "knn_model.fit(X, y)\n",
        "\n",
        "knn_pred = knn_model.predict(X_new)\n",
        "print(\"KNN Regression prediction (Cyprus):\", knn_pred[0][0])"
      ]
    },
